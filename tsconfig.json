{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["./src/"],
  "exclude": ["node_modules"]
}
